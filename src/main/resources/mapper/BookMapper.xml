<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ybvtc.mapper.BookMapper">

    <resultMap id="BaseResultMap" type="com.ybvtc.domain.entity.Book">
            <id property="id" column="book_id" jdbcType="INTEGER"/>
            <result property="name" column="book_name" jdbcType="VARCHAR"/>
            <result property="isbn" column="book_isbn" jdbcType="VARCHAR"/>
            <result property="press" column="book_press" jdbcType="VARCHAR"/>
            <result property="author" column="book_author" jdbcType="VARCHAR"/>
            <result property="pagination" column="book_pagination" jdbcType="INTEGER"/>
            <result property="price" column="book_price" jdbcType="DOUBLE"/>
            <result property="uploadtime" column="book_uploadtime" jdbcType="VARCHAR"/>
            <result property="status" column="book_status" jdbcType="VARCHAR"/>
            <result property="borrower" column="book_borrower" jdbcType="VARCHAR"/>
            <result property="borrowtime" column="book_borrowtime" jdbcType="VARCHAR"/>
            <result property="returntime" column="book_returntime" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="Base_Column_List">
        book_id,book_name,book_isbn,
        book_press,book_author,book_pagination,
        book_price,book_uploadtime,book_status,
        book_borrower,book_borrowtime,book_returntime
    </sql>
</mapper>
